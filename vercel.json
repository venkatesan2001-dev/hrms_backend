{
  "version": 2,
  "builds": [
    {
      "src": "../main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "../main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"],
      "headers": { "Access-Control-Allow-Origin": "*" }
    }
  ],
  "outputDirectory": "dist"
}
